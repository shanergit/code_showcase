    "key = 5\n",
    "\n",
    "def encrypt_text(text,key):\n",
    "  enc_text = \"\"\n",
    "  for letter in text:\n",
    "    ascii = ord(letter)\n",
    "    ascii += key\n",
    "    enc_text += chr(ascii)\n",
    "  return enc_text\n",
    "\n",
